<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="trinat.hexenspiel.hexenspiel.DifficultyController">
   <children>
      <BorderPane layoutX="24.0" prefHeight="400.0" prefWidth="546.0">
         <left>
            <Button fx:id="buttonEasy" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleEasyButtonAction" text="Easy" BorderPane.alignment="CENTER">
               <font>
                  <Font size="37.0" />
               </font>
            </Button>
         </left>
         <center>
            <Button fx:id="buttonMedium" mnemonicParsing="false" onAction="#handleMediumButtonAction" text="Medium" BorderPane.alignment="CENTER">
               <font>
                  <Font size="37.0" />
               </font>
            </Button>
         </center>
         <right>
            <Button fx:id="buttonHard" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleHardButtonAction" text="Hard" BorderPane.alignment="CENTER">
               <font>
                  <Font size="37.0" />
               </font>
            </Button>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
